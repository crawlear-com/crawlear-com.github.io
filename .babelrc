// .babelrc
{
    "presets": [
      "@babel/preset-react",
      [
       "@babel/preset-env",
        {
          "targets": {
            "browsers": "last 2 versions"
          },
          "modules": false,
          "loose": false
        }
      ]
    ],
    "plugins": [
      "transform-class-properties"
    ]
}